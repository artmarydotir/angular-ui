doctype html
html(data-ng-app="app", dir="rtl", lang="fa")
  head
    meta(charset="utf-8")
    meta(name="robots", content="noindex, nofollow")
    meta(http-equiv="x-ua-compatible", content="ie=edge")
    title(data-ng-bind-template="{{ title }} | " + manifest.name) بارگزاری...
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    link(rel="manifest", href="app.webmanifest")
    link(rel="shortcut icon", href="favicon.ico", type="image/x-icon")
    link(rel="icon", href="favicon.ico", type="image/x-icon")

    each i in icons
      link(rel="icon", href="lib/icons/icon-" + i.s +".png", sizes=i.size)

    link(rel="apple-touch-icon-precomposed", href="lib/icons/icon-152.png", sizes="152x152")
    style(type="text/css").
      body{overflow:hidden}@keyframes mainloading{0%,100%{opacity:0}50%{opacity:1}}#loading{display:block;position:fixed;top:0;left:0;width:100%;height:100%;bottom:0;right:0;background-color:#fff;z-index:66000}#loading div{background-repeat:no-repeat;background-position:center center;background-image:url(data:image/png;base64,);position:absolute;top:50%;left:50%;width:128px;height:128px;margin-top:-64px;margin-left:-64px;-webkit-animation:mainloading .6s linear infinite;-moz-animation:mainloading .6s linear infinite;-o-animation:mainloading .6s linear infinite;animation:mainloading .6s linear infinite}
  body
    div(id="loading")
      div
    div(id="progress", data-ng-show!="progress > 0")
      .progress
        .progress-bar.progress-bar-success(role="progressbar", aria-valuemin="{{ progress }}", aria-valuemax="100", style="width: {{ progress }}%")
    div(id="wrapper", data-ng-view="")
    link(rel="stylesheet", type="text/css", href="lib/css/main.css")
    script(src="lib/js/framework.min.js")

    if pretty
      script(src="lib/js/main.pack.js")
    else
      script(src="lib/js/main.min.js")
